<div id='header'>
  <div class='home-link'>
    <%= link_to root_path  do %>
      <i class="logo fa fa-cubes fa-3x"></i>
    <% end %>
  </div>
  <div class='header-links'>
    <div class="link-container">
      <%= link_to "Home", root_path %>
    </div>
    <div class='link-container'>
      <%= link_to "About", about_path %>
    </div>
    <div class='link-container'>
      <%= link_to "Portfolio", portfolio_path %>
    </div>
    <div class='link-container'>
      <%= link_to "Contact Me", contact_me_path %>
    </div>
    <div class='link-container'>
      <%= link_to "Blog", 'https://contycontent.wordpress.com/' %>
    </div>
  </div>
  <div class="mobile-hamburger"><i class="fa fa-bars fa-3x"></i></div>
  <div class='mobile-links'>
    <%= link_to "About", about_path %>
    <%= link_to "Portfolio", portfolio_path %>
    <%= link_to "Contact Me", contact_me_path  %>
    <%= link_to "Blog", 'https://contycontent.wordpress.com/' %>
  </div>
</div>
