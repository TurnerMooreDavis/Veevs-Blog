<div id='social-media'>
  <img src='/facebook-symbol.png' class="icon" data-name="facebook"></img>
  <img src='/twitter-link.png' class="icon" data-name='twitter'></img>
  <img src='/linkedin-logo.png' class="icon" data-name='linkedin'></img>
  <img src='/google-reg-trademark.png' class="icon" data-name='google'></img>
  <div class='hidden-buttons'>
    <div id="facebook-button" class='social-media'>
      <%= facebook_button %>
    </div>
    <div id="linkedin-button" class='social-media'>
    <%= linkedin_button %>
  </div>
  <div id="twitter-button" class='social-media'>
    <%= twitter_button %>
  </div>
  <div id="google-button" class='social-media'>
    <%= google_plus_button %>
  </div>
  </div>
</div>
