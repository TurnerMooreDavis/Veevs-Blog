<p id="notice"><%= notice %></p>

<h1>Listing Samples</h1>

<table>
  <thead>
    <tr>
      <th>Category</th>
      <th>Primery keyword</th>
      <th>Second keyword</th>
      <th>Third keyword</th>
      <th>Host url</th>
      <th>Company website</th>
      <th>Reference urls</th>
      <th>Notes</th>
      <th>Email</th>
      <th>Price</th>
      <th>Word count</th>
      <th>Topic</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @samples.each do |sample| %>
      <tr>
        <td><%= sample.category_id %></td>
        <td><%= sample.primery_keyword %></td>
        <td><%= sample.second_keyword %></td>
        <td><%= sample.third_keyword %></td>
        <td><%= sample.host_url %></td>
        <td><%= sample.company_website %></td>
        <td><%= sample.reference_urls %></td>
        <td><%= sample.notes %></td>
        <td><%= sample.email %></td>
        <td><%= sample.price %></td>
        <td><%= sample.word_count %></td>
        <td><%= sample.topic %></td>
        <td><%= link_to 'Show', sample %></td>
        <td><%= link_to 'Edit', edit_sample_path(sample) %></td>
        <td><%= link_to 'Destroy', sample, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sample', new_sample_path %>
